    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.6.1",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
